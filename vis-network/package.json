{
  "name": "vis-network",
  "version": "0.0.0-no-version",
  "description": "A dynamic, browser-based visualization library (PRSM patched version - peer build only).",
  "homepage": "https://visjs.github.io/vis-network/",
  "license": "(Apache-2.0 OR MIT)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visjs/vis-network.git"
  },
  "keywords": [
    "vis",
    "visualization",
    "graph",
    "network"
  ],
  "source": "./lib/entry-peer.ts",
  "main": "./peer/esm/vis-network.mjs",
  "module": "./peer/esm/vis-network.mjs",
  "types": "./declarations/index.d.ts",
  "exports": {
    ".": {
      "import": "./peer/esm/vis-network.mjs",
      "types": "./declarations/index.d.ts"
    },
    "./peer": {
      "import": "./peer/esm/vis-network.mjs",
      "types": "./declarations/index.d.ts"
    },
    "./package.json": "./package.json",
    "./declarations/index.d.ts": {
      "types": "./declarations/index.d.ts"
    },
    "./styles/vis-network.css": "./peer/esm/vis-network.css"
  },
  "targets": {
    "main": {
      "source": "./lib/entry-peer.ts",
      "distDir": "./peer/esm",
      "outputFormat": "esmodule",
      "isLibrary": true,
      "sourceMap": true
    }
  },
  "files": [
    "LICENSE*",
    "declarations",
    "peer"
  ],
  "type": "module",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ],
  "scripts": {
    "build": "npm run build:declarations && npm run build:code",
    "build:declarations": "shx mkdir -p declarations && shx cp -r \"types/*\" declarations && tsc -p tsconfig.declarations.json",
    "build:code": "parcel build",
    "build:watch": "parcel watch",
    "clean": "shx rm -rf \"declarations\" \"peer\" \".parcel-cache\""
  },
  "dependencies": {
    "uuid": "^11.0.0"
  },
  "peerDependencies": {
    "component-emitter": "^1.3.0 || ^2.0.0",
    "keycharm": "^0.2.0 || ^0.3.0 || ^0.4.0",
    "vis-data": ">=8.0.0",
    "vis-util": ">=6.0.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-tsc": "^2.16.4",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "component-emitter": "1.3.1",
    "keycharm": "0.4.0",
    "parcel": "^2.12.0",
    "shx": "0.4.0",
    "typescript": "^5.3.0",
    "vis-data": "8.0.3",
    "vis-util": "6.0.0"
  }
}
